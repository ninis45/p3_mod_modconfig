<Window xmlns:WPF="clr-namespace:Steema.TeeChart.WPF;assembly=TeeChart.WPF" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModelosConfig"          
        xmlns:lang="clr-namespace:ModelosConfig.My.Resources"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="ConfigView"
        mc:Ignorable="d"
        Title="{Binding Titulo}" Height="800" Width="820" WindowState="Normal" ResizeMode="NoResize"   >

    <Window.Resources>



        <DataTemplate x:Key="TemplateMecanico">


            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="22"></RowDefinition>
                    <RowDefinition Height="18"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="40"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="40"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="40"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="{Binding ModPozoTuberia.ETIQUETA}" Foreground="DarkCyan" FontSize="14" Grid.Column="0" Grid.Row="0" FontWeight="Bold" Grid.ColumnSpan="3"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="MD" Grid.Column="0" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.MD,StringFormat={}{0:0.00}}"  Grid.Column="1" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold" Text="TIDIAM" Grid.Column="2" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.TIDIAM,StringFormat={}{0:0.00}}" Grid.Column="3" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="TODIAM" Grid.Column="4" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.TODIAM,StringFormat={}{0:0.00}}" Grid.Column="5" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="CIDIAM" Grid.Column="6" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.CIDIAM,StringFormat={}{0:0.00}}" Grid.Column="7" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="AE" Grid.Column="8" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding AE,StringFormat={}{0:0.00}}" Grid.Column="9" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="DE" Grid.Column="10" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding DE,StringFormat={}{0:0.00}}" Grid.Column="11" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="DH" Grid.Column="12" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding DH,StringFormat={}{0:0.00}}" Grid.Column="13" Grid.Row="1"></TextBlock>
            </Grid>



        </DataTemplate>
        <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
        <telerik:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
        <telerik:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter"/>
        <telerik:NumberToVisibilityConverter x:Key="NumberToVisibilityConverter"/>
        <local:CombosViewModel x:Key="WRCombos"/>

        <local:NumToBool x:Key="WRNumToBool"></local:NumToBool>
        <local:ToVisibility x:Key="WRVisibility"></local:ToVisibility>
        <local:NullToBolean x:Key="RWNullToBoolean"></local:NullToBolean>
        <local:InverseBoolean x:Key="WRInverseBoolean"></local:InverseBoolean>
        <!--<local:Translation x:Key="WRTranslation"></local:Translation>-->
        <CollectionViewSource Source="{Binding Bombas}" x:Key="CVSBombas">
            <!--<CollectionViewSource.SortDescriptions>
                <scm:SortDescription PropertyName="Apellido"/>
                
            </CollectionViewSource.SortDescriptions>-->
            
            <CollectionViewSource.GroupDescriptions>
                <PropertyGroupDescription PropertyName="FABRICANTE">
                    
                </PropertyGroupDescription>
                
            </CollectionViewSource.GroupDescriptions>
        </CollectionViewSource>
        <Style x:Key="GStyleSelector">
            
        </Style>
    </Window.Resources>

    <Grid Background="#FFE5E5E5">
        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="../Resources/Notices.xaml" />
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <telerik:RadTabControl HorizontalAlignment="Stretch"  Grid.Row="0"  BorderThickness="0" Margin="15"   >
            <telerik:RadTabItem x:Name="ItemGral" Header="Generales" FontFamily="Century Gothic" FontSize="14" Background="#FF918F8F"  >
                <!--Background="#FF6A8570"-->
                <StackPanel Orientation="Vertical" Margin="10">
                    <Border Style="{StaticResource alert-warning}" Visibility="{Binding Errors.Count,Converter={StaticResource NumberToVisibilityConverter}}">
                        <TextBlock Text="Existen observaciones y/o faltantes de datos, favor de solventar o corregir dicha observaciones."/>
                    </Border>
                    <Grid >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>


                        </Grid.RowDefinitions>


                        <Grid Margin="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="130"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="130"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>

                            </Grid.RowDefinitions>

                            <Label Grid.Column="0" Grid.Row="0" xmlns:local="clr-namespace:ModelosConfig.My.Resources" Content="{x:Static local:Resources.FLUID}"/>
                            <ComboBox   SelectedIndex="{Binding ModGeneral.FLUID}" Height="22" Grid.Column="1" Grid.Row="0">
                                <ComboBoxItem Content="Aceite y Agua"></ComboBoxItem>
                                <ComboBoxItem Content="Gas húmedo y seco"></ComboBoxItem>
                                <ComboBoxItem Content="Gas retrógrado"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="0" Content="{x:Static lang:Resources.COMPLETION}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.COMPLETION}"  Grid.Column="3" Grid.Row="0" Height="22" >
                                <ComboBoxItem Content="Agujero entubado"></ComboBoxItem>
                                <ComboBoxItem Content="Agujero descubierto"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="1" xmlns:local="clr-namespace:ModelosConfig.My.Resources"  Content="{x:Static local:Resources.PVTMODEL}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.PVTMODEL}" Grid.Column="1" Grid.Row="1" Height="22">
                                <ComboBoxItem Content="Aceite negro"></ComboBoxItem>
                                <ComboBoxItem Content="Ecuación de estado"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="1" Content="{x:Static lang:Resources.GRAVELPACK}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.GRAVELPACK}" Grid.Column="3" Grid.Row="1" Height="22">
                                <ComboBoxItem Content="No"></ComboBoxItem>
                                <ComboBoxItem Content="Empacamiento de grava"></ComboBoxItem>
                                <ComboBoxItem Content="Sedazo"></ComboBoxItem>
                                <ComboBoxItem Content="Tubería ranurada con pantalla"></ComboBoxItem>
                                <ComboBoxItem Content="Liner ranurado"></ComboBoxItem>
                            </ComboBox>



                            <Separator Grid.Row="2" Grid.ColumnSpan="4" ></Separator>

                            <Label Grid.Column="0" Grid.Row="3"  Content="{x:Static lang:Resources.SEPARATOR}" />
                            <ComboBox SelectedIndex="{Binding ModGeneral.SEPARATOR}" Grid.Column="1" Grid.Row="3" Height="22" SelectedValuePath="Key">
                                <ComboBoxItem Content="Una etapa"></ComboBoxItem>
                                <ComboBoxItem Content="Dos etapas"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="3" Content="{x:Static lang:Resources.INFLOWTYPE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.INFLOWTYPE}" Grid.Column="3" Grid.Row="3" Height="22" >
                                <ComboBoxItem Content="Una rama"></ComboBoxItem>
                                <ComboBoxItem Content="Pozo Multilateral"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="4" Content="{x:Static lang:Resources.EMULSION}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.EMULSION}" Grid.Column="1" Grid.Row="4" Height="22">
                                <ComboBoxItem Content="No"></ComboBoxItem>
                                <ComboBoxItem Content="Emulsión + corrección de viscosidad"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="4" Content="{x:Static lang:Resources.GASCONING}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.GASCONING}" Grid.Column="3" Grid.Row="4" Height="22">
                                <ComboBoxItem Content="No"></ComboBoxItem>
                                <ComboBoxItem Content="Si"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="5" Content="{x:Static lang:Resources.HYDRATE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.HYDRATE}" Grid.Column="1" Grid.Row="5" Height="22">
                                <ComboBoxItem Content="Desactivar advertencia"></ComboBoxItem>
                                <ComboBoxItem Content="Activar advertencia"></ComboBoxItem>
                            </ComboBox>
                            <Separator Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="2" Margin="10 0 0 0"></Separator>

                            <Label Grid.Column="0" Grid.Row="6" Content="{x:Static lang:Resources.WATVIS}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.WATVIS}" Grid.Column="1" Grid.Row="6" Height="22">
                                <ComboBoxItem Content="Usar correlación por defecto"></ComboBoxItem>
                                <ComboBoxItem Content="Usar correlación de presión corregida"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="6" Content="{x:Static lang:Resources.DATGENDATE}"/>
                            <telerik:RadDateTimePicker  SelectedValue="{Binding  ModGeneral.DATGENDATE,Mode=TwoWay}"   Grid.Column="3" Grid.Row="6" Height="22" />

                            <Label Grid.Column="0" Grid.Row="7" Content="{x:Static lang:Resources.VISMOD}" />
                            <ComboBox SelectedIndex="{Binding VisMod}" Grid.Column="1" Grid.Row="7" Height="22" >
                                <ComboBoxItem Content="Fluido Newtoniano"></ComboBoxItem>
                                <ComboBoxItem Content="Fluido no Newtoniano"></ComboBoxItem>
                            </ComboBox>
                           
                            





                            <Separator Grid.Column="0" Grid.Row="8" Grid.ColumnSpan="2"></Separator>

                            <Label Grid.Column="0" Grid.Row="9" Content="{x:Static lang:Resources.FLOWTYPE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.FLOWTYPE}"   Grid.Column="1" Grid.Row="9" Height="22" Margin="0,4" >
                                <ComboBoxItem Content="Flujo por aparejo"></ComboBoxItem>
                                <ComboBoxItem Content="Flujo por anular"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="10" Content="{x:Static lang:Resources.WELLTYPE}"/>
                            <ComboBox  SelectedIndex="{Binding ModGeneral.WELLTYPE}" Grid.Column="1" Grid.Row="10" Height="22" >
                                <ComboBoxItem Content="Productor"></ComboBoxItem>
                                <ComboBoxItem Content="Inyector"></ComboBoxItem>
                                <ComboBoxItem Content="Inyector de agua"></ComboBoxItem>
                            </ComboBox>

                            <Separator Grid.Column="0" Grid.Row="11" Grid.ColumnSpan="2"></Separator>

                            <Label Grid.Column="0" Grid.Row="12" Content="{x:Static lang:Resources.LIFTMETHOD}"/>
                            <ComboBox SelectedIndex="{Binding LiftMethod}"  Grid.Column="1" Grid.Row="12" Height="22" >
                                <ComboBoxItem Content="Ninguno"></ComboBoxItem>
                                <ComboBoxItem Content="Bombeo neumático (Contínuo)"></ComboBoxItem>
                                <ComboBoxItem Content="Bombeo Electrocentrífugo"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="13" Content="{x:Static lang:Resources.LIFTTYPE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.LIFTYPE}"  Grid.Column="1" Grid.Row="13" Height="22" >
                                <ComboBoxItem Content="Sin pérdida de fricción en anular"></ComboBoxItem>
                                <ComboBoxItem Content="Pérdidas por fricción en anular"></ComboBoxItem>
                                <ComboBoxItem Content="Equipo de seguridad"></ComboBoxItem>
                            </ComboBox>

                            <Separator Grid.Column="0" Grid.Row="14" Grid.ColumnSpan="2"></Separator>

                            <Label Grid.Column="0" Grid.Row="15" Content="{x:Static lang:Resources.PREDICT}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.PREDICT}"  Grid.Column="1" Grid.Row="15" Height="22" >
                                <ComboBoxItem Content="Sólo presión"></ComboBoxItem>
                                <ComboBoxItem Content="Presión y temperatura (Costa afuera)"></ComboBoxItem>
                                <ComboBoxItem Content="Presión y temperatura (Costa adentro)"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="16" Content="{x:Static lang:Resources.TEMPMODEL}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.TEMPMODEL}"  Grid.Column="1" Grid.Row="16" Height="22" >
                                <ComboBoxItem Content="Aproximada"></ComboBoxItem>
                                <ComboBoxItem Content="Balance de Entalpía"></ComboBoxItem>
                                <ComboBoxItem Content="Aproximación mejorada"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="17" Content="{x:Static lang:Resources.RANGESYSTEM}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.RANGESYSTEM}"  Grid.Column="1" Grid.Row="17" Height="22" >
                                <ComboBoxItem Content="Todo el Sistema"></ComboBoxItem>
                                <ComboBoxItem Content="Sólo tubería"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="18" xmlns:local="clr-namespace:ModelosConfig.My.Resources" Content="{x:Static local:Resources.OUTPUTRES}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.OUTPUTRES}"  Height="22" Grid.Column="1" Grid.Row="18">
                                <ComboBoxItem Content="Mostrar datos calculados"></ComboBoxItem>
                                <ComboBoxItem Content="No mostrar datos calculados"></ComboBoxItem>
                            </ComboBox>






                        </Grid>


                        <Grid Grid.Row="1" Margin="10" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="4*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="50"></RowDefinition>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>


                            <Label Grid.Column="0" Grid.Row="0"  Content="Comentarios"/>
                            <TextBox Text="{Binding Comenta}" Grid.Column="1" Grid.Row="0" ></TextBox>


                            <!--<StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" Margin="0 10 0 0"   >

                                <CheckBox Content="Actualizar Edo. Mecánico" IsChecked="{Binding DelMecanico}"></CheckBox>

                                <CheckBox Content="Actualizar trayectoria" IsChecked="{Binding DelTrayectoria}"></CheckBox>

                                <CheckBox Content="Actualizar temperatura" IsChecked="{Binding DelTemperatura}"></CheckBox>

                            </StackPanel>-->
                        </Grid>




                    </Grid>
                </StackPanel>

            </telerik:RadTabItem>
            <telerik:RadTabItem Header="PVT" FontFamily="Century Gothic" FontSize="14" BorderBrush="#FF959595" Background="#FF8BA28F" HeaderForeground="#FF022927"   >
                <StackPanel Orientation="Vertical" Margin="10">
                    <Border Style="{StaticResource alert-info}" >
                        <TextBlock TextWrapping="Wrap"   Text="Puedes cargar un archivo PVT(.Out) o si lo prefieres selecciona una fecha de muestreo para crear un nuevo."/>
                    </Border>
                    <Grid >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="26"></RowDefinition>
                            <RowDefinition Height="26"></RowDefinition>
                            <RowDefinition Height="36"></RowDefinition>
                        </Grid.RowDefinitions>

                        <Label Content="Archivo PVT(.out)" Grid.Row="0" Grid.ColumnSpan="3" FontWeight="Bold"></Label>

                        <TextBox IsReadOnly="True" Grid.Column="0" Grid.Row="1" Text="{Binding NewArchivoPvt}" Visibility="{Binding ElementName=txtFileName,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}"></TextBox>
                        <TextBlock Padding="4 2" Grid.Column="0" Background="#EFEFEF" Grid.Row="1" Text="{Binding ArchivoPvt}" Visibility="{Binding ArchivoPvt,Converter={StaticResource NullToVisibilityConverter}}" Name="txtFileName"></TextBlock>
                        <Button Grid.Column="1"  Grid.Row="1" Background="DarkCyan" BorderThickness="0" Content="Examinar" Click="SelectFile" Foreground="White" Margin="1 0" Visibility="{Binding ElementName=txtFileName,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}"></Button>
                        <Button Grid.Column="2"  Grid.Row="1" Background="Gray"  BorderThickness="0" Content="Eliminar" Click="DeleteFile" Foreground="White" Margin="1 0" Visibility="{Binding ArchivoPvt,Converter={StaticResource NullToVisibilityConverter}}"></Button>

                        <Button Grid.Column="2"  Grid.Row="1" Background="Gray"  BorderThickness="0" Content="Borrar" Click="ClearFile" Foreground="White" Margin="1 0" Visibility="{Binding ElementName=txtFileName,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}"></Button>

                        <CheckBox IsEnabled="{Binding EnabledPvt,Converter={StaticResource WRInverseBoolean}}" Name="chkEquipment" IsChecked="{Binding EnabledEquip}" Grid.Row="2" Grid.ColumnSpan="2" Content="Actualizar equipamiento(Edo. Mecanico, Trayectorias y Temperaturas)" Margin="0 10"></CheckBox>
                    </Grid>
                    <Separator></Separator>
                    <Grid IsEnabled="{Binding EnabledPvt}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="180"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="180"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"></RowDefinition>
                            <RowDefinition Height="30"></RowDefinition>
                            <RowDefinition Height="30"></RowDefinition>
                            <RowDefinition Height="10"></RowDefinition>

                            <RowDefinition  Height="30"></RowDefinition>
                            <RowDefinition  Height="30"></RowDefinition>
                            <RowDefinition  Height="30"></RowDefinition>
                            <RowDefinition  Height="auto"></RowDefinition>

                        </Grid.RowDefinitions>

                        <Label Content="Fecha muestreo" Grid.Column="0" Grid.Row="0"></Label>
                        <telerik:RadComboBox Height="22" Grid.Column="1" Grid.Row="0" EmptyText=" [ Seleccionar ]" ItemsSource="{Binding Formaciones}" DisplayMemberPath="FECHADEMUESTREO" SelectedItem="{Binding Formacion,Mode=TwoWay}"></telerik:RadComboBox>
                        <Label Content="API" Grid.Column="2" Grid.Row="0"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.API}"  Height="22" Grid.Column="3" Grid.Row="0" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>

                        <Label Content="Temperatura" Grid.Column="0" Grid.Row="1"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.TEMP}" Height="22" Grid.Column="1" Grid.Row="1" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>
                        <Label Content="Densidad" Grid.Column="2" Grid.Row="1"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.DENSIDAD}" Height="22" Grid.Column="3" Grid.Row="1" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>

                        <Label Content="Salinidad" Grid.Column="0" Grid.Row="2"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.SALINIDAD}" Height="22" Grid.Column="1" Grid.Row="2" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>
                        <Label Content="GOR" Grid.Column="2" Grid.Row="2"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.GOR}" Height="22" Grid.Column="3" Grid.Row="2" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>

                        <Separator Grid.Row="3" Grid.ColumnSpan="4"></Separator>

                        <Label Content="CO2" Grid.Column="0" Grid.Row="4"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.CO2}" Height="22" Grid.Column="1" Grid.Row="4" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>
                        <Label Content="H2S" Grid.Column="2" Grid.Row="4"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.H2S}" Height="22" Grid.Column="3" Grid.Row="4" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>

                        <Label Content="N2" Grid.Column="0" Grid.Row="5"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.N2}" Height="22" Grid.Column="1" Grid.Row="5" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>
                        <Label Content="DRG" Grid.Column="2" Grid.Row="5"></Label>
                        <telerik:RadNumericUpDown Value="{Binding Formacion.DRG}" Height="22" Grid.Column="3" Grid.Row="5" ShowButtons="False" IsReadOnly="True"></telerik:RadNumericUpDown>



                        <Grid Grid.Row="8" Grid.ColumnSpan="4">

                            <telerik:RadExpander  Header="Gráfica" ExpandDirection="Left" >
                                <WindowsFormsHost Name="wfhPvts"></WindowsFormsHost>
                            </telerik:RadExpander>
                            <ScrollViewer VerticalScrollBarVisibility="Visible" Height="400" Margin="0 0  30 0" >
                                <telerik:RadGridView  ItemsSource="{Binding Pvts}" IsReadOnly="True" AutoGenerateColumns="False">
                                    <telerik:RadGridView.Columns >
                                        <telerik:GridViewDataColumn Header="PPRUEBA" DataMemberBinding="{Binding PPRUEBA}"  DataFormatString="0.00"/>
                                        <telerik:GridViewDataColumn Header="PB" DataMemberBinding="{Binding PB}"  DataFormatString="0.00"/>

                                        <telerik:GridViewDataColumn Header="RS" DataMemberBinding="{Binding RS}"  DataFormatString="0.00"/>
                                        <telerik:GridViewDataColumn Header="BO" DataMemberBinding="{Binding BO}"  DataFormatString="0.00"/>
                                        <telerik:GridViewDataColumn Header="OV" ToolTip="Viscosidad del aceite" DataMemberBinding="{Binding VISCOSIDADACEITE}"  DataFormatString="0.00"/>
                                    </telerik:RadGridView.Columns>

                                </telerik:RadGridView>

                            </ScrollViewer>


                        </Grid>



                    </Grid>
                </StackPanel>

            </telerik:RadTabItem>
            <telerik:RadTabItem Name="ItemYacimiento" Header="Yacimiento" FontFamily="Century Gothic" FontSize="14" Background="#FF8BA28F" HeaderForeground="#FF022927">
                <ScrollViewer>
                    <Grid Margin="10">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="*"/>

                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>



                        <Label Grid.Column="0" Grid.Row="0" Content="{x:Static lang:Resources.IPRMETHOD}"/>
                        <ComboBox SelectedIndex="{Binding ModGeneral.IPRMETHOD,Mode=TwoWay}" Grid.Column="1" Grid.Row="0" Height="22"  Name="ddlIPRMethod">
                            <ComboBoxItem Content="Entrada IP"></ComboBoxItem>
                            <ComboBoxItem Content="Vogel"></ComboBoxItem>
                            <ComboBoxItem Content="Compuesto" IsEnabled="False"></ComboBoxItem>
