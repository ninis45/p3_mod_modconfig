<Window xmlns:WPF="clr-namespace:Steema.TeeChart.WPF;assembly=TeeChart.WPF" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModelosConfig"          
        xmlns:lang="clr-namespace:ModelosConfig.My.Resources"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="ConfigView"
        mc:Ignorable="d"
        Title="{Binding Titulo}" Height="820" Width="880" WindowState="Normal"   >

    <Window.Resources>

        <DataTemplate x:Key="TemplateMecanico">


            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="22"></RowDefinition>
                    <RowDefinition Height="18"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="40"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="40"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="40"></ColumnDefinition>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="{Binding ModPozoTuberia.ETIQUETA}" Foreground="DarkCyan" FontSize="12" Grid.Column="0" Grid.Row="0" FontWeight="Bold" Grid.ColumnSpan="3"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="MD" Grid.Column="0" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.MD,StringFormat={}{0:0.00}}"  Grid.Column="1" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold" Text="TIDIAM" Grid.Column="2" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.TIDIAM,StringFormat={}{0:0.00}}" Grid.Column="3" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="TODIAM" Grid.Column="4" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.TODIAM,StringFormat={}{0:0.00}}" Grid.Column="5" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="CIDIAM" Grid.Column="6" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding ModPozoTuberia.CIDIAM,StringFormat={}{0:0.00}}" Grid.Column="7" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="AE" Grid.Column="8" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding AE,StringFormat={}{0:0.00}}" Grid.Column="9" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="DE" Grid.Column="10" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding DE,StringFormat={}{0:0.00}}" Grid.Column="11" Grid.Row="1"></TextBlock>

                <TextBlock FontSize="11" FontWeight="Bold"  Text="DH" Grid.Column="12" Grid.Row="1"></TextBlock>
                <TextBlock FontSize="11" Text="{Binding DH,StringFormat={}{0:0.00}}" Grid.Column="13" Grid.Row="1"></TextBlock>
            </Grid>



        </DataTemplate>
        <telerik:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"></telerik:BooleanToVisibilityConverter>
        <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
        <telerik:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
        <telerik:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter"/>
        <telerik:NumberToVisibilityConverter x:Key="NumberToVisibilityConverter"/>
        <local:CombosViewModel x:Key="WRCombos"/>

        <local:NumToBool x:Key="WRNumToBool"></local:NumToBool>
        <local:ToVisibility x:Key="WRVisibility"></local:ToVisibility>
        <local:NullToBolean x:Key="RWNullToBoolean"></local:NullToBolean>
        <local:InverseBoolean x:Key="WRInverseBoolean"></local:InverseBoolean>
        <local:EqualVisibility x:Key="WREqualVisibility"></local:EqualVisibility>
        <!--<local:Translation x:Key="WRTranslation"></local:Translation>-->
        <CollectionViewSource Source="{Binding Bombas}" x:Key="CVSBombas">
            <!--<CollectionViewSource.SortDescriptions>
                <scm:SortDescription PropertyName="Apellido"/>
                
            </CollectionViewSource.SortDescriptions>-->
            
            <CollectionViewSource.GroupDescriptions>
                <PropertyGroupDescription PropertyName="FABRICANTE">
                    
                </PropertyGroupDescription>
                
            </CollectionViewSource.GroupDescriptions>
        </CollectionViewSource>
        <Style TargetType="Button" x:Key="Btn">
            <Setter Property="FontFamily" Value="Century Gothic"></Setter>
            <Setter Property="FontSize" Value="14"></Setter>
            <Setter Property="Width" Value="100"></Setter>
            <Setter Property="Padding" Value="2"></Setter>
            <Setter Property="Margin" Value="2"></Setter>
            <Setter Property="BorderThickness" Value="0"></Setter>
        </Style>
    </Window.Resources>

    <Grid Background="#FFE5E5E5">
        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="../Resources/Notices.xaml" />
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Grid.Resources>
        <Grid.RowDefinitions>
            
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>

        
        <telerik:RadTabControl HorizontalAlignment="Stretch"  Grid.Row="0"  BorderThickness="0" Margin="15"    >
            <telerik:RadTabItem x:Name="ItemGral" Header="Generales" FontFamily="Century Gothic" FontSize="14" Background="#FF918F8F" IsSelected="{Binding Tab,Converter= {StaticResource WRNumToBool},ConverterParameter=1}"  >
                <ScrollViewer>
                    <StackPanel Orientation="Vertical" Margin="10">
                        <!--<Border Style="{StaticResource alert-warning}" Visibility="{Binding Warnings.Count,Converter={StaticResource NumberToVisibilityConverter}}">
                            <TextBlock Text="Existen observaciones y/o faltantes de datos, favor de solventar o corregir dicha observaciones."/>
                        </Border>-->
                        <telerik:RadBusyIndicator IsBusy="{Binding IsBusy}" BusyContent="Comunicando con el servidor.." Background="Transparent" BorderThickness="0" Foreground="DarkCyan" >


                            <Grid Margin="4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="26"></RowDefinition>
                                    <RowDefinition Height="26"></RowDefinition>
                                    <RowDefinition Height="36"></RowDefinition>
                                </Grid.RowDefinitions>

                                <Label Content="Archivo (.Out)" Grid.Row="0" Grid.ColumnSpan="3" FontWeight="Bold"></Label>

                                <TextBox IsReadOnly="True" Grid.Column="0" Grid.Row="1" Text="{Binding NewArchivoPvt}" Visibility="{Binding ElementName=txtFileName,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}" Name="txtPath"></TextBox>
                                <TextBlock Padding="4 2" Grid.Column="0" Background="#EFEFEF" Grid.Row="1" Text="{Binding ModArchivo.nombreArchivo}" Visibility="{Binding ModArchivo,Converter={StaticResource NullToVisibilityConverter}}" Name="txtFileName"></TextBlock>



                                <Button Grid.Column="1"  Grid.Row="1" Background="DarkCyan" BorderThickness="0" Content="Examinar" Click="SelectFile" Foreground="White" Margin="1 0" Visibility="{Binding ElementName=txtFileName,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}"></Button>

                                <Button Grid.Column="1"  Grid.Row="1" Background="#e74b3c" IsEnabled="{Binding ElementName=pnlEdit,Path=IsVisible}"  BorderThickness="0" Content="Eliminar" Click="DeleteFile" Foreground="White" Margin="1 0" Visibility="{Binding ModArchivo,Converter={StaticResource NullToVisibilityConverter}}"></Button>
                                <Button Grid.Column="2" Grid.Row="1" Background="#999999" BorderThickness="0"  Foreground="White" Click="DownloadFile">
                                    <Button.Content>
                                        <StackPanel Orientation="Horizontal">
                                            <telerik:RadGlyph Glyph="&#xe132;"></telerik:RadGlyph>
                                            <TextBlock Text="Descargar"></TextBlock>
                                        </StackPanel>
                                    </Button.Content>
                                </Button>

                                <Button Grid.Column="2"  Grid.Row="1" Background="Gray"  BorderThickness="0" Content="Borrar" Click="ClearFile" Foreground="White" Margin="1 0"   Visibility="{Binding ElementName=txtFileName,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}"></Button>

                                <CheckBox IsEnabled="{Binding EnabledPvt,Converter={StaticResource WRInverseBoolean}}" Name="chkEquipment" IsChecked="{Binding EnabledEquip}" Grid.Row="2" Grid.ColumnSpan="2" Content="Extraer datos del  archivo.Out y guardarlo en la Base de Datos." Margin="0 10" ToolTip="Actualizar directamente del archivo PVT(.Out) hacia la base de datos"></CheckBox>
                            </Grid>
                        </telerik:RadBusyIndicator>
                        <Separator ></Separator>

                        
                        <Grid Margin="4" IsEnabled="{Binding ElementName=pnlEdit,Path=IsVisible}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="130"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="130"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                            </Grid.RowDefinitions>

                            <Label Grid.Column="0" Grid.Row="0" xmlns:local="clr-namespace:ModelosConfig.My.Resources" Content="{x:Static local:Resources.FLUID}"/>
                            <ComboBox   SelectedIndex="{Binding ModGeneral.FLUID}" Height="24" Grid.Column="1" Grid.Row="0" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Aceite y Agua"></ComboBoxItem>
                                <ComboBoxItem Content="Gas húmedo y seco"></ComboBoxItem>
                                <ComboBoxItem Content="Gas retrógrado"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="0" Content="{x:Static lang:Resources.COMPLETION}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.COMPLETION}"  Grid.Column="3" Grid.Row="0" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Agujero entubado"></ComboBoxItem>
                                <ComboBoxItem Content="Agujero descubierto"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="1" xmlns:local="clr-namespace:ModelosConfig.My.Resources"  Content="{x:Static local:Resources.PVTMODEL}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.PVTMODEL}" Grid.Column="1" Grid.Row="1" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Aceite negro"></ComboBoxItem>
                                <ComboBoxItem Content="Ecuación de estado"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="1" Content="{x:Static lang:Resources.GRAVELPACK}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.GRAVELPACK}" Grid.Column="3" Grid.Row="1" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="No"></ComboBoxItem>
                                <ComboBoxItem Content="Empacamiento de grava"></ComboBoxItem>
                                <ComboBoxItem Content="Cedazo"></ComboBoxItem>
                                <ComboBoxItem Content="Tubería ranurada con pantalla"></ComboBoxItem>
                                <ComboBoxItem Content="Liner ranurado"></ComboBoxItem>
                            </ComboBox>



                            <Separator Grid.Row="2" Grid.ColumnSpan="4" ></Separator>

                            <Label Grid.Column="0" Grid.Row="3"  Content="{x:Static lang:Resources.SEPARATOR}" />
                            <ComboBox SelectedIndex="{Binding ModGeneral.SEPARATOR}" Grid.Column="1" Grid.Row="3" Height="24" SelectedValuePath="Key" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Una etapa"></ComboBoxItem>
                                <ComboBoxItem Content="Dos etapas"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="3" Content="{x:Static lang:Resources.INFLOWTYPE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.INFLOWTYPE}" Grid.Column="3" Grid.Row="3" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}" >
                                <ComboBoxItem Content="Una rama"></ComboBoxItem>
                                <ComboBoxItem Content="Pozo Multilateral"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="4" Content="{x:Static lang:Resources.EMULSION}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.EMULSION}" Grid.Column="1" Grid.Row="4" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="No"></ComboBoxItem>
                                <ComboBoxItem Content="Emulsión + corrección de viscosidad"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="4" Content="{x:Static lang:Resources.GASCONING}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.GASCONING}" Grid.Column="3" Grid.Row="4" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="No"></ComboBoxItem>
                                <ComboBoxItem Content="Si"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="5" Content="{x:Static lang:Resources.HYDRATE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.HYDRATE}" Grid.Column="1" Grid.Row="5" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Desactivar advertencia"></ComboBoxItem>
                                <ComboBoxItem Content="Activar advertencia"></ComboBoxItem>
                            </ComboBox>
                            <Separator Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="2" Margin="10 0 0 0"></Separator>

                            <Label Grid.Column="0" Grid.Row="6" Content="{x:Static lang:Resources.WATVIS}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.WATVIS}" Grid.Column="1" Grid.Row="6" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Usar correlación por defecto"></ComboBoxItem>
                                <ComboBoxItem Content="Usar correlación de presión corregida"></ComboBoxItem>
                            </ComboBox>
                            <Label Grid.Column="2" Grid.Row="6" Content="{x:Static lang:Resources.DATGENDATE}"/>
                            <telerik:RadDateTimePicker  SelectedValue="{Binding  ModGeneral.DATGENDATE,Mode=TwoWay}"   Grid.Column="3" Grid.Row="6" Height="26" telerik:StyleManager.Theme="Fluent" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}" />

                            <Label Grid.Column="0" Grid.Row="7" Content="{x:Static lang:Resources.VISMOD}" />
                            <ComboBox SelectedIndex="{Binding ModGeneral.VISMOD}" Grid.Column="1" Grid.Row="7" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Fluido Newtoniano"></ComboBoxItem>
                                <ComboBoxItem Content="Fluido no Newtoniano"></ComboBoxItem>
                            </ComboBox>







                            <Separator Grid.Column="0" Grid.Row="8" Grid.ColumnSpan="2"></Separator>

                            <Label Grid.Column="0" Grid.Row="9" Content="{x:Static lang:Resources.FLOWTYPE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.FLOWTYPE}"   Grid.Column="1" Grid.Row="9" Height="24" Margin="0,4" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Flujo por aparejo"></ComboBoxItem>
                                <ComboBoxItem Content="Flujo por anular"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="10" Content="{x:Static lang:Resources.WELLTYPE}"/>
                            <ComboBox  SelectedIndex="{Binding ModGeneral.WELLTYPE}" Grid.Column="1" Grid.Row="10" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Productor"></ComboBoxItem>
                                <ComboBoxItem Content="Inyector"></ComboBoxItem>
                                <ComboBoxItem Content="Inyector de agua"></ComboBoxItem>
                            </ComboBox>

                            <Separator Grid.Column="0" Grid.Row="11" Grid.ColumnSpan="2"></Separator>

                            <Label Grid.Column="0" Grid.Row="12" Content="{x:Static lang:Resources.LIFTMETHOD}"/>
                            <ComboBox SelectedIndex="{Binding LiftMethod}"  Grid.Column="1" Grid.Row="12" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Ninguno"></ComboBoxItem>
                                <ComboBoxItem Content="Bombeo neumático (Contínuo)"></ComboBoxItem>
                                <ComboBoxItem Content="Bombeo Electrocentrífugo"></ComboBoxItem>
                            </ComboBox>


                            <Label Grid.Column="0" Grid.Row="13" Content="{x:Static lang:Resources.LIFTTYPE}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.LIFTYPE}"  Grid.Column="1" Grid.Row="13" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Sin pérdida de fricción en anular"></ComboBoxItem>
                                <ComboBoxItem Content="Pérdidas por fricción en anular"></ComboBoxItem>
                                <ComboBoxItem Content="Equipo de seguridad"></ComboBoxItem>
                            </ComboBox>

                            <Separator Grid.Column="0" Grid.Row="14" Grid.ColumnSpan="2"></Separator>

                            <Label Grid.Column="0" Grid.Row="15" Content="{x:Static lang:Resources.PREDICT}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.PREDICT}"  Grid.Column="1" Grid.Row="15" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Sólo presión"></ComboBoxItem>
                                <ComboBoxItem Content="Presión y temperatura (Costa afuera)"></ComboBoxItem>
                                <ComboBoxItem Content="Presión y temperatura (Costa adentro)"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="16" Content="{x:Static lang:Resources.TEMPMODEL}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.TEMPMODEL}"  Grid.Column="1" Grid.Row="16" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Aproximada"></ComboBoxItem>
                                <ComboBoxItem Content="Balance de Entalpía"></ComboBoxItem>
                                <ComboBoxItem Content="Aproximación mejorada"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="17" Content="{x:Static lang:Resources.RANGESYSTEM}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.RANGESYSTEM}"  Grid.Column="1" Grid.Row="17" Height="24" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Todo el Sistema"></ComboBoxItem>
                                <ComboBoxItem Content="Sólo tubería"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="18" xmlns:local="clr-namespace:ModelosConfig.My.Resources" Content="{x:Static local:Resources.OUTPUTRES}"/>
                            <ComboBox SelectedIndex="{Binding ModGeneral.OUTPUTRES}"  Height="24" Grid.Column="1" Grid.Row="18" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                                <ComboBoxItem Content="Mostrar datos calculados"></ComboBoxItem>
                                <ComboBoxItem Content="No mostrar datos calculados"></ComboBoxItem>
                            </ComboBox>

                            <Label Grid.Column="0" Grid.Row="19"  Content="Comentarios"/>
                            <TextBox Text="{Binding Comenta}" Grid.Column="1" Grid.Row="19" Grid.ColumnSpan="4" Grid.RowSpan="2" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}"></TextBox>






                        </Grid>
                    </StackPanel>
                </ScrollViewer>
            </telerik:RadTabItem>
            <telerik:RadTabItem x:Name="ItemAforo" Header="Aforos" FontFamily="Century Gothic" FontSize="14" Background="#FF8BA28F" HeaderForeground="#FF022927"  IsSelected="{Binding Tab,Converter= {StaticResource WRNumToBool},ConverterParameter=4}" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                <StackPanel Margin="10">
                    <Border Style="{StaticResource alert-warning}" Visibility="{Binding Aforos.Count,Converter={StaticResource WREqualVisibility},ConverterParameter=0}" Name="msgAforo">
                        <TextBlock TextWrapping="Wrap" Text="Actualmente no hay registros de Aforos. Se necesita completar la información inicial en la pestaña Condiciones de Operación."></TextBlock>
                    </Border>
                    <Grid Visibility="{Binding Aforos.Count, Converter={StaticResource NumberToVisibilityConverter}}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="600"></RowDefinition>
                        </Grid.RowDefinitions>
                        <telerik:RadExpander Grid.Column="0" Header="Gráfica" ExpandDirection="Left" >
                            <WindowsFormsHost Name="wfhAforos"></WindowsFormsHost>
                        </telerik:RadExpander>
                        <telerik:RadGridView  ItemsSource="{Binding Aforos,Mode=TwoWay}" AutoGenerateColumns="False" IsReadOnly="True"  Grid.Column="0" Margin="0 0  30 0" telerik:StyleManager.Theme="Windows8Touch" ShowGroupPanel="False">
                            <telerik:RadGridView.Columns >

                                <telerik:GridViewDataColumn Header="FECHA" DataMemberBinding="{Binding FECHA}" DataFormatString="dd/MM/yyyy" />
                                <telerik:GridViewDataColumn Header="Ql" DataMemberBinding="{Binding PRODLIQ}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Qgf" DataMemberBinding="{Binding PRODGASFORM}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Qgi" DataMemberBinding="{Binding VOLGASINY}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Ptp1" DataMemberBinding="{Binding PTP1}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Ptp2" DataMemberBinding="{Binding PTP2}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Ptr1" DataMemberBinding="{Binding PTR1}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Ptr2" DataMemberBinding="{Binding PTR2}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="% Agua" DataMemberBinding="{Binding FRACAGUA}" IsReadOnly="True"/>
                                <telerik:GridViewDataColumn Header="Temp" DataMemberBinding="{Binding TEMP}" IsReadOnly="True"/>
                            </telerik:RadGridView.Columns>
                            <telerik:EventToCommandBehavior.EventBindings>
                                <telerik:EventBinding
                                            Command="{Binding OnLoadAforo}"
                                            EventName="MouseDoubleClick"
                                            PassEventArgsToCommand="True"/>
                            </telerik:EventToCommandBehavior.EventBindings>
                        </telerik:RadGridView>



                    </Grid>
                </StackPanel>



            </telerik:RadTabItem>
            <telerik:RadTabItem Header="PVT" FontFamily="Century Gothic" FontSize="14" BorderBrush="#FF959595" Background="#FF8BA28F" HeaderForeground="#FF022927"  IsSelected="{Binding Tab,Converter= {StaticResource WRNumToBool},ConverterParameter=2}" IsEnabled="{Binding ElementName=chkEquipment,Path=IsChecked,Converter={StaticResource WRInverseBoolean}}">
                <StackPanel Orientation="Vertical" Margin="10" >
                    
                    <Border Style="{StaticResource alert-info}" Visibility="{Binding ElementName=pnlEdit,Path=IsVisible,Converter={StaticResource BooleanToVisibilityConverter}}">
                        <TextBlock TextWrapping="Wrap"   Text="Puedes cargar un archivo PVT(.Out) o si lo prefieres selecciona una fecha de muestreo para crear un nuevo."/>
                    </Border>
                    <Border Style="{StaticResource alert-warning}" Visibility="{Binding ElementName=grdPvt,Path=IsVisible,Converter={StaticResource InvertedBooleanToVisibilityConverter}}" Name="msgPvt">
                        <TextBlock TextWrapping="Wrap" Text="No hay registros de PVTs y/o formaciones. Talvés sea  necesario seleccionar un archivo PVT(.Out)  para guardar la configuración."></TextBlock>
                    </Border>
                    <!--<Border Style="{StaticResource alert-warning}" Visibility="{Binding ModArchivo,Converter={StaticResource NullToVisibilityConverter}}" Name="msgAlert">
                        <StackPanel>
                            <TextBlock TextWrapping="Wrap"   Text="Archivo PVT(.Out) precargado o generado durante la ejecución de PROSPER."/>
                            
                        </StackPanel>
                       
                    </Border>-->
                    
                   
                    <Grid IsEnabled="{Binding EnabledPvt}" Visibility="{Binding Formaciones.Count,Converter={StaticResource NumberToVisibilityConverter}}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="180"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="30"></ColumnDefinition>
                            <ColumnDefinition Width="180"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"></RowDefinition>
